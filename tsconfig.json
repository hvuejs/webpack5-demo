{
  "compilerOptions": {
    "lib": [
        "ES5",
        "DOM",
        "DOM.Iterable"
    ],
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "types",
    "strict": true,
    // "noEmit":true, // 不输出文件，只做类型检查
    "suppressImplicitAnyIndexErrors": true // 支持 el["onclick"] 这种写法
  },
  "files": ["src/index.ts"],
  "exclude": ["node_modules", "dist"]
}
